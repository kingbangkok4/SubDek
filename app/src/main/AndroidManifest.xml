<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.subdek">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainMenuActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AbountActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AlphabetActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DayActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MonthsActivity" android:screenOrientation="portrait"/>
        <activity android:name=".NumberActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ColorActivity" android:screenOrientation="portrait"/>
        <activity android:name=".DisplayActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>
